apiVersion: machineconfiguration.openshift.io/v1
kind: MachineConfig
metadata:
  labels:
    machineconfiguration.openshift.io/role: "{{ cnf_nodes_role }}"
  name: load-xt-u32-module
spec:
  config:
    ignition:
      version: 3.2.0
    storage:
      files:
        - contents:
            source: data:text/plain;charset=utf-8,xt_u32
          filesystem: root
          mode: 420
          path: /etc/modules-load.d/xt_u32-load.conf
